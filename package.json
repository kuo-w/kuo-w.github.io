{
    "name": "kuo-w.github.io",
    "version": "1.0.0",
    "description": "Portfolio site",
    "main": "index.html",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "parcel build index.html --public-url ./",
        "postbuild": "react-snap && copyfiles README.md dist",
        "dev": "parcel index.html",
        "watch": "parcel watch index.html",
        "deploy": "gh-pages -b master -d dist",
        "cp-readme": "cp README.md dist/",
        "snap": "react-snap"
    },
    "reactSnap": {
        "source": "dist",
        "puppeteerArgs": [
            "--no-sandbox",
            "--disable-setuid-sandbox"
        ]
    },
    "homepage": "https://kuo-w.github.io",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kuo-w/kuo-w.github.io.git"
    },
    "keywords": [
        "William Kuo"
    ],
    "author": "William Kuo",
    "license": "MIT",
    "dependencies": {
        "assemble": "^0.3.83",
        "fs": "0.0.1-security",
        "grunt": "^0.4.5",
        "js-yaml": "^3.13.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-transition-group": "^4.1.0",
        "src": "^1.1.2",
        "styles": "^0.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "copyfiles": "^2.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-react": "^7.19.0",
        "gh-pages": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "react-snap": "^1.23.0",
        "rimraf": "^3.0.2",
        "sass": "^1.26.5"
    }
}
