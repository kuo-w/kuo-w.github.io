  },
  "homepage": "https://github.com/kuo-w/kuo-w.github.io#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "js-yaml": "^3.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-slidedown": "^2.4.1",
    "react-snap": "^1.23.0",
    "react-transition-group": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
